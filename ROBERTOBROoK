local Main = MakeTab({Name = "الاعب Player"})
AddTextBox(Main, {
  Name = "سرعة Speed",
  Default = "",
  PlaceholderText = "حط الرقم",
  ClearText = true,
  Callback = function(value)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value     
 end
})
AddTextBox(Main, {
  Name = "قفز Jump",
  Default = "",
  PlaceholderText = "حط الرقم",
  ClearText = true,
  Callback = function(value)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = value    
  end
})

