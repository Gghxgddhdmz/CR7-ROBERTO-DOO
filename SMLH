Tab:AddButton({
	Name = "ANTI BAN",
	Callback = function()
      	for _, v in pairs(getconnections(plr.Idled)) do
			v:Disable()
		end

		-- تعطيل kick / Ban / Shutdown
		for _, v in pairs(getgc(true)) do
			if typeof(v) == "function" and islclosure(v) then
				local info = debug.getinfo(v)
				if info.name == "kick" or info.name == "Ban" or info.name == "Shutdown" then
					hookfunction(v, function() return end)
				end
			end
		end

		-- تعطيل الريموتات المشبوهة
		for _, obj in pairs(game:GetDescendants()) do
			if obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction") then
				local name = obj.Name:lower()
				if name:find("ban") or name:find("report") or name:find("cheat") then
					obj:Destroy()
				end
			end
		end

		-- منع destroy لـ LocalPlayer أو CoreGui
		local mt = getrawmetatable(game)
		setreadonly(mt, false)
		local old = mt.__namecall

		mt.__namecall = newcclosure(function(self, ...)
			local method = getnamecallmethod()
			if (method == "Kick" or method == "Destroy") and (self == plr or tostring(self):lower():find("coregui")) then
				return nil
			end
	        return old(self, ...)
		end    
})
