local function getmaxvalue(val)
    local mainvalueifonetable = 5000 -- ØªÙ‚Ù„ÙŠÙ„ Ø§Ù„Ù‚ÙŠÙ…Ø© Ø§Ù„Ø£Ø³Ø§Ø³ÙŠØ©
    if type(val) ~= "number" then
